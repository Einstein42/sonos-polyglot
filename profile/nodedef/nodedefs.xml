<nodeDefs>
    <!-- Sonos Control -->
    <nodeDef id="sonoscontrol" nls="sctl">
        <editors />
        <sts />
        <cmds>
            <sends />
            <accepts>
			    <cmd id="DISCOVER" />
		    </accepts>
        </cmds>
    </nodeDef>
	<!-- Sonos Speakers -->
    <nodeDef id="sonosspeaker" nls="sspk">
        <editors />
        <sts>
            <st id="ST" editor="sspkst" />
        </sts>
        <cmds>
            <sends />
            <accepts>
                <cmd id="PLAY" />
                <cmd id="STOP" />
            </accepts>
        </cmds>
    </nodeDef>
</nodeDefs>